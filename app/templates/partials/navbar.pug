include mixins.pug


div.menu
	div.logo
		a(href="index.html")
			img(src="images/logo.svg", alt="")
	nav.nav_menu
		ul.navbar-nav
			each item in pages
				li.nav-item 
					a.nav-link(href=item + ".html") #{item}
				
				
				

	nav.langer
		ul.navbar-nav.lang
			li.nav-item
				a.nav-link.active(href="index.html") CZ
			li.nav-item
				a.nav-link(href="index.html") EN